var myMembers = (function() {
	return {
		members: [],
		storeMembers: function(arg) {
			this.members = arg;
		},
		getMemeberById: function(id) {
			var toRet = null;
			for(var x = 0; x < this.members.length; x++) {
				if(this.members[x].id == id) {
					toRet = this.members[x];
				}
			}
		}
	};
})();