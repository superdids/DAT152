"use strict";

(function() {

	function lastet() {
		document.getElementById("newMember").addEventListener("click", newMember, false);
		
		var url = servicesPath.path + "/endringer";
		var ajax = new AJAXConnection(url);
		ajax.onsuccess = members;
		ajax.get(-1);
	};
	
	function members(m) {
		var a = JSON.parse(m);
		alert(m.updates);
		for(var key in a) {
			console.log(key);
		}
	}
	
	function newMember() {
		//TODO
		alert(3);
	}
	
	window.addEventListener("DOMContentLoaded", lastet, false);
})();