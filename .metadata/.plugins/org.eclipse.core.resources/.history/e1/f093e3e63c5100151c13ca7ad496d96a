"use strict";

var Person = function(a,b) {
	var name = a || "";
	
	return {
		comp: b || "",
		getName: function() {
			return name;
		},
		setName: function(n) {
			name = n;
		}
	}
	
};


var p1 = Person("a", "b");
var p2 = Person("c", "d");

//alert(p1.name); //"undefined", m√• bruke getName();
//alert(p1.getName()); //"a"
//alert(p2.comp); //"d"