"use strict";

var symbols = [];
var rows = 15;
var cols = 75;
window.onload = function() {
	var elm = document.getElementById('digital');
	elm.rows = rows;
	elm.cols = cols;
	
	for(var x = 0; x < 50; x++) {
		elm.value += "w";
	}
}

function rand(arg) {
	return Math.floor(Math.random() * arg);
}




function generateSymbols() {
	var symb = new Array();
	for(var x = 0; x < 10; x++) {
		symb[x] = x;
	}
}