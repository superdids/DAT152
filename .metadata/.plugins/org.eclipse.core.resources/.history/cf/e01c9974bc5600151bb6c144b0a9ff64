//Regex

//\( \) - match TEGNENE parentes
//(sin|cos) - gruppering, match forekomsten sin eller cos
//[] - finn forekomsten av ett tegn her


(function() {
	var reg = /(sin|cos)\s*\([.0-9]+\)/i;

	console.log(reg.test("sin(0.9)"));
})();


(function() {
	var reg = /(sin|cos)\s*\([.0-9]+\)/i;

	console.log(reg.test("sin(0.9)"));
})();