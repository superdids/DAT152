"use strict";

var logId = {
	value: -1	
};

(function() {
	function lastet() {
		
//		function refresh() {
////			console.log("hai");
//			window.setTimeout(function() {
//				var http = new HTTPOperations("/endringer", updateLogId);
//				http.getMembersByLogId(logId.value);
//				console.log("hai");
//				refresh();
//			}, 5000);	
//		}
		
		window.setInterval(function() {
			var http = new HTTPOperations("/endringer", updateLogId);
			http.getMembersByLogId(logId.value);
			console.log("hai");
		},1000);
		
		function updateLogId(responseText) {
			var obj = JSON.parse(responseText);
			obj = obj.updates;
			logId.value = obj.logId;
			console.log(logId.value);
		}
	}

	window.addEventListener("DOMContentLoaded", lastet);
})();