"use strict";

(function() {
	var lastet = function() {
		function forsinketUtskrift(tekst) {
			window.setTimeout(window.alert(tekst), 1000);
		}
		forsinketUtskrift("En liten utskrift");
	}
	
	window.addEventListener('load', lastet, false);
})();


function delay() {
	var func = function() { alert(3); };
	window.setTimeout(func, 1000);
}

deleay