//Oppgave 2
function a() {
	return parseInt("1000", 2);
}

function b() {
	return parseInt("100", 2);
}

//AND
console.log(a() & b());

//OR
console.log(a() | b());

//XOR
console.log(a() ^ b());

alert(8 & 8 );