function Vindu(path, parent) {
	this._width = 100;
	this._height = 100;
	this._path = path || null;
	this._parent = parent || null;
}

Vindu.prototype.open = function() {
	try {
		this.window.open(this._path);
	} catch(e) {
		console.error(e);
	}
}

Vindu.prototype.close = function() {
	this.window.close();
}


